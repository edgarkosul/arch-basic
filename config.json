{
    "audio": "pulseaudio",
    "bootloader": "grub-install",
    "filesystem": "btrfs",
    "gfx_driver": "Nvidia",
    "harddrives": [
        "/dev/nvme0n1p4"
    ],
    "swap": true,
    "hostname": "arch-linux",
    "kernels": [
        "linux-zen"
    ],
    "keyboard-language": "us,ru",
    "mirror-region": "Worldwide",
    "nic": {
        "NetworkManager": true,
    },
    "ntp": true,
    "packages": ["git", "wget", "zsh", "thinkfun"],
    "profile": "kde",
    "services": ["thinkfun"],
    "sys-encoding": "utf-8",
    "sys-language": "en_US",
    "timezone": "Europe/Moscow",
}